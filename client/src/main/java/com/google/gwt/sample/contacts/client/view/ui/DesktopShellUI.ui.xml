<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:style>
    .DetailsPanel {
    }

    .MasterPanel {
    }
  </ui:style>

  <g:DockLayoutPanel unit='EM'>
    <g:center>
      <g:SimplePanel ui:field="_detailsPanel" addStyleNames="{style.DetailsPanel}"/>
    </g:center>
    <g:west size='20'>
      <g:SimplePanel ui:field="_masterPanel" addStyleNames="{style.MasterPanel}"/>
    </g:west>
  </g:DockLayoutPanel>
</ui:UiBinder>
